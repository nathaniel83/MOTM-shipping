<% include ./header %>




<div class="login-page">
  
  <div class="form">
    <% if(msg&&success){ %>
      <p class="msgcolor"><%= msg %></p>
      <% } %>
      <% if(msg&&!success){ %>
        <p class="badmsg"><%= msg %></p>
        <% } %>
      <h5>File Upload Portal Login</h5>
    <form action="/authenticate" method="post" class="login-form">
        
      <input type="text" name="username" placeholder="username"/>
      <input type="password" name="password" placeholder="password"/>
      
      <button type="submit">Log in</button>
      <p class="message">Are you a customer? <a href="http://www.mailonthemoveshipping.com/customer">Track Shipping</a></p>
    </form>
  
  </div>
</div>

